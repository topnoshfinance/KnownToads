# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Farcaster Configuration
FARCASTER_API_KEY=your_farcaster_api_key
TOADGOD_FID=1234567

# Base Chain Configuration
BASE_RPC_URL=https://mainnet.base.org
NEXT_PUBLIC_BASE_CHAIN_ID=8453

# 0x Protocol Configuration (optional, for higher rate limits)
ZEROX_API_KEY=your_0x_api_key

# Zora API Configuration
# Get your API key from: https://zora.co/developers
# Used for swapping Zora creator coins via Uniswap V4
ZORA_API_KEY=your_zora_api_key

# Optional: Override Zora API endpoints if defaults are incorrect
# ZORA_POOL_API_BASE_URL=https://api.zora.co/pools
# ZORA_SWAP_API_BASE_URL=https://api-sdk.zora.engineering

# Uniswap V4 Configuration (Optional - for direct V4 swaps)
# PoolManager contract address on Base
# If not set, direct V4 swaps will be skipped in routing
# Example: NEXT_PUBLIC_UNISWAP_V4_POOL_MANAGER=0x...
NEXT_PUBLIC_UNISWAP_V4_POOL_MANAGER=

# Optional: Uniswap V4 Quoter for gas estimates (not yet implemented)
# Example: NEXT_PUBLIC_UNISWAP_V4_QUOTER=0x...
NEXT_PUBLIC_UNISWAP_V4_QUOTER=

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
